{
  "name": "kotor",
  "version": "1.0.0",
  "description": "KotOR JS - A remake of the Odyssey Game Engine that powered KotOR I & II",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w dxt",
    "webpack:library": "webpack",
    "webpack:library-watch": "webpack --watch",
    "webpack:launcher": "webpack --config ./webpack.launcher.config.js",
    "webpack:launcher-watch": "webpack --watch --config ./webpack.launcher.config.js",
    "webpack:game": "webpack --config ./webpack.game.config.js",
    "webpack:game-watch": "webpack --watch --config ./webpack.game.config.js",
    "webpack:forge": "webpack --config ./webpack.forge.config.js",
    "webpack:forge-watch": "webpack --watch --config ./webpack.forge.config.js",
    "webpack:dev-watch": "npm-run-all --parallel webpack:launcher-watch webpack:game-watch webpack:forge-watch"
  },
  "author": "KobaltBlu",
  "license": "GPL-3.0",
